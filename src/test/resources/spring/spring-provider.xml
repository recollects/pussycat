<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/mvc
        http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <!-- 提供方配置 -->
    <bean id="userServiceProvider" class="com.alipay.pussycat.insidetest.model.UserServiceImpl"/>
    <bean class="com.alipay.pussycat.spring.support.PussycatServerBean">
        <property name="serviceName" value="com.alipay.pussycat.insidetest.model.UserService"/>
        <property name="target" ref="userServiceProvider"/>
    </bean>


</beans>    